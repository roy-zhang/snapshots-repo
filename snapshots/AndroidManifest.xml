<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="sensing.snapShots"
      android:versionCode="1"
      android:versionName="1.0">


    <uses-sdk android:minSdkVersion="9"
              android:targetSdkVersion="17"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name="sensing.snapShots.MainActivity" android:screenOrientation="landscape"
                  android:label="@string/app_name"
                  android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
        <activity android:name="sensing.snapShots.ProfileSelectionActivity" android:screenOrientation="landscape"></activity>
        <activity android:name="sensing.snapShots.GraphActivity" android:screenOrientation="landscape"></activity>
    </application>
</manifest>